#include <iostream>
using namespace std;

int main() {
    int t;
    cin>>t;
    while(t--){
        int n,w;
        cin>>n;
        cin>>w;
        int val[n],wt[n];
        for(int i=0;i<n;i++)cin>>val[i];
        for(int i=0;i<n;i++)cin>>wt[i];
        
	int dp[n+1][w+1];
	for(int i=0;i<=n;i++)
	for(int j=0;j<=w;j++){
	    if(j==0||i==0)
	    dp[i][j]=0;
	    else if(j>=wt[i-1])
	    dp[i][j]=max(dp[i-1][j],dp[i-1][j-wt[i-1]]+val[i-1]);
	    else
	    dp[i][j]=dp[i-1][j];
	}
	cout<<dp[n][w]<<endl;
    }
	return 0;
}
